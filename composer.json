{
    "name": "drupal-10-compatibility/warmer",
    "description": "Drupal 10 Compatibility:  warmer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/warmer",
    "repositories": {
        "drupal/warmer": {
            "type": "package",
            "package": {
                "name": "drupal/warmer",
                "version": "dev-main",
                "type": "drupal-module",
                "source": {
                    "url": "https://git.drupalcode.org/issue/warmer-3309120.git",
                    "type": "git",
                    "reference": "drupal/warmer"
                }
            }
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/classy": "^1.0",
        "drupal/core": "9.5.x-dev",
        "drupal/core-composer-scaffold": "9.5.x-dev",
        "drupal/core-recommended": "9.5.x-dev",
        "drupal/upgrade_status": "^3.14",
        "drupal/warmer": "dev-main#09c7782838f5a64303666c8f6b3cadf5c59dc00f",
        "drush/drush": "^11.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "require-dev": {
        "drupal/core-dev": "9.5.x-dev",
        "phpspec/prophecy-phpunit": "^2",
        "phpstan/phpstan-deprecation-rules": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.1 as 7.0"
        },
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "composer/installers": true,
            "wikimedia/composer-merge-plugin": true,
            "oomphinc/composer-installers-extender": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "file-mapping": {
                "[web-root]/robots.txt": false,
                "[web-root]/.ht.router.php": false,
                "[web-root]/example.gitignore": false,
                "[web-root]/.htaccess": false,
                "[web-root]/INSTALL.txt": false,
                "[web-root]/LICENSE.txt": false,
                "[web-root]/README.txt": false,
                "[web-root]/web.config": false
            },
            "initial": {
                "sites/default/default.services.yml": "docroot/sites/default/default.services.yml",
                "sites/default/default.settings.php": "docroot/sites/default/default.settings.php",
                "robots.txt": "docroot/robots.txt",
                ".htaccess": "docroot/.htaccess"
            },
            "omit-defaults": true,
            "locations": {
                "web-root": "docroot/"
            }
        },
        "installer-types": [
            "component",
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset"
            ]
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {}
    },
    "autoload": {
        "psr-4": {
            "Drupal10\\": "src/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@putenv SHELL_INTERACTIVE=1",
            "Drupal10\\ComposerScripts::interactiveConfiguration",
            "@composer update"
        ],
        "post-install-cmd": [],
        "post-update-cmd": []
    }
}
